<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/dashboard_bg"
    tools:context=".patient.home.PatientDashBoardFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="75dp">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Update Availability"
                android:textSize="25dp"
                android:layout_gravity="center"
                android:textAlignment="center"
                android:fontFamily="@font/poppins_medium"
                android:textColor="@color/black"/>
        </FrameLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_marginLeft="62dp"
            android:layout_marginTop="30dp"
            android:background="@drawable/rounded_corners"
            android:padding="10dp"
            android:layout_marginRight="62dp">

            <TextView
                android:id="@+id/current_schedule"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAlignment="center"
                android:background="@android:color/transparent"
                android:fontFamily="@font/poppins_regular"
                android:text="Current Schedule"
                android:textSize="15sp" />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="62dp"
            android:layout_marginTop="30dp"
            android:background="@drawable/rounded_corners"
            android:padding="10dp"
            android:layout_marginRight="62dp">

            <TextView
                android:id="@+id/next_availability_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:fontFamily="@font/poppins_regular"
                android:textAlignment="center"
                android:text="New Availability"
                android:textSize="15sp" />

        </RelativeLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:layout_marginTop="10sp"
            android:layout_marginLeft="62dp"
            android:layout_marginRight="62dp">
            <TextView
                android:id="@+id/mon_FN"
                android:layout_width="0dp"
                android:layout_weight="0.75"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corners"
                android:fontFamily="@font/poppins_regular"
                android:text="Monday FN"
                android:layout_marginRight="5dp"
                android:textAlignment="center"
                android:padding="5sp"
                android:textSize="15sp" />
            <TextView
                android:id="@+id/mon_AN"
                android:layout_width="0dp"
                android:layout_weight="0.75"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corners"
                android:fontFamily="@font/poppins_regular"
                android:text="Monday AN"
                android:padding="5sp"
                android:textSize="15sp" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:layout_marginTop="10sp"
            android:layout_marginLeft="62dp"
            android:layout_marginRight="62dp">
            <TextView
                android:id="@+id/tue_FN"
                android:layout_width="0dp"
                android:layout_weight="0.75"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corners"
                android:fontFamily="@font/poppins_regular"
                android:text="Tuesday FN"
                android:layout_marginRight="5dp"
                android:textAlignment="center"
                android:padding="5sp"
                android:textSize="15sp" />
            <TextView
                android:id="@+id/tue_AN"
                android:layout_width="0dp"
                android:layout_weight="0.75"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corners"
                android:fontFamily="@font/poppins_regular"
                android:text="Tuesday AN"
                android:padding="5sp"
                android:textSize="15sp" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:layout_marginTop="10sp"
            android:layout_marginLeft="62dp"
            android:layout_marginRight="62dp">
            <TextView
                android:id="@+id/wed_FN"
                android:layout_width="0dp"
                android:layout_weight="0.75"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corners"
                android:fontFamily="@font/poppins_regular"
                android:text="Wednesday FN"
                android:layout_marginRight="5dp"
                android:textAlignment="center"
                android:padding="5sp"
                android:textSize="15sp" />
            <TextView
                android:id="@+id/wed_AN"
                android:layout_width="0dp"
                android:layout_weight="0.75"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corners"
                android:fontFamily="@font/poppins_regular"
                android:text="Wednesday AN"
                android:padding="5sp"
                android:textSize="15sp" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:layout_marginTop="10sp"
            android:layout_marginLeft="62dp"
            android:layout_marginRight="62dp">

            <TextView
                android:id="@+id/thu_FN"
                android:layout_width="0dp"
                android:layout_weight="0.75"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corners"
                android:fontFamily="@font/poppins_regular"
                android:text="Thursday FN"
                android:layout_marginRight="5dp"
                android:textAlignment="center"
                android:padding="5sp"
                android:textSize="15sp" />
            <TextView
                android:id="@+id/thu_AN"
                android:layout_width="0dp"
                android:layout_weight="0.75"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corners"
                android:fontFamily="@font/poppins_regular"
                android:text="Thursday AN"
                android:padding="5sp"
                android:textSize="15sp" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:layout_marginTop="10sp"
            android:layout_marginLeft="62dp"
            android:layout_marginRight="62dp">
            <TextView
                android:id="@+id/fri_FN"
                android:layout_width="0dp"
                android:layout_weight="0.75"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corners"
                android:fontFamily="@font/poppins_regular"
                android:text="Friday FN"
                android:layout_marginRight="5dp"
                android:textAlignment="center"
                android:padding="5sp"
                android:textSize="15sp" />
            <TextView
                android:id="@+id/fri_AN"
                android:layout_width="0dp"
                android:layout_weight="0.75"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corners"
                android:fontFamily="@font/poppins_regular"
                android:text="Friday AN"
                android:padding="5sp"
                android:textSize="15sp" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:layout_marginTop="10sp"
            android:layout_marginLeft="62dp"
            android:layout_marginRight="62dp">
            <TextView
                android:id="@+id/sat_FN"
                android:layout_width="0dp"
                android:layout_weight="0.75"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corners"
                android:fontFamily="@font/poppins_regular"
                android:text="Saturday FN"
                android:layout_marginRight="5dp"
                android:textAlignment="center"
                android:padding="5sp"
                android:textSize="15sp" />
            <TextView
                android:id="@+id/sat_AN"
                android:layout_width="0dp"
                android:layout_weight="0.75"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corners"
                android:fontFamily="@font/poppins_regular"
                android:text="Saturday AN"
                android:padding="5sp"
                android:textSize="15sp" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:layout_marginTop="10sp"
            android:layout_marginLeft="62dp"
            android:layout_marginRight="62dp">
            <TextView
                android:id="@+id/sun_FN"
                android:layout_width="0dp"
                android:layout_weight="0.75"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corners"
                android:fontFamily="@font/poppins_regular"
                android:text="Sunday FN"
                android:layout_marginRight="5dp"
                android:textAlignment="center"
                android:padding="5sp"
                android:textSize="15sp" />
            <TextView
                android:id="@+id/sun_AN"
                android:layout_width="0dp"
                android:layout_weight="0.75"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corners"
                android:fontFamily="@font/poppins_regular"
                android:text="Sunday AN"
                android:padding="5sp"
                android:textSize="15sp" />
        </LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="62dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/rounded_login_corners"
            android:padding="10dp"
            android:layout_marginRight="62dp">

            <ImageView
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:src="@drawable/ic_addtime"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp" />

            <TextView
                android:id="@+id/update_availability"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:fontFamily="@font/poppins_regular"
                android:text="Update Availability"
                android:textColor="@color/white"
                android:textAlignment="center"
                android:textSize="15sp" />

        </RelativeLayout>
    </LinearLayout>

</FrameLayout>